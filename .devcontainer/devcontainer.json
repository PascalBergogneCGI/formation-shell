{
  "name": "cgichaos-devcontainer",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": true,
      "username": "coder",
      "userUid": 1000,
      "userGid": 1000
    }
  },
  "postCreateCommand": "code-server --list-extensions",
  "customizations": {
    "vscode": {
      "extensions": [
        "yzhang.markdown-all-in-one"
      ]
    }
  }
}
